################################
####### GENERAL PARAMETERS ######
#################################
max_step = 18000

amr.n_cell = n_cellx n_celly n_cellz
amr.max_grid_size = max_grid_size
amr.blocking_factor = blocking_factor
amr.refine_grid_layout = 1  # if n_MPI > n_grids, the grids will be successively divided in half until n_MPI <= n_grids

# need this here for some reason with blocking_factor = 40
amr.check_input = 0

geometry.dims = 3
geometry.prob_lo = -Lx/2. 0. 0.
geometry.prob_hi =  Lx/2. Ly Lz

amr.max_level = 0

# use pec instead of pml overlaying current source so you don't get a reflection
boundary.field_lo = pml pml pec
boundary.field_hi = pml pml pml

#################################
############ NUMERICS ###########
#################################
warpx.verbose = 1

warpx.cfl = 0.9

# vacuum or macroscopic
algo.em_solver_medium = macroscopic

# laxwendroff or backwardeuler
algo.macroscopic_sigma_method = laxwendroff

###############
# geometry
# each row represents a different part of the circuit
# 1. air everywhere, then add in alumina
# 2. alumina at the bottom of the microstrip
# 2. metal above alumina (aluminum or copper)
###############

macroscopic.sigma_function(x,y,z) = "sigma_air
+ (sigma_alumina - sigma_air)   * (z >= 0) * (z < sub_thick)"

macroscopic.epsilon_function(x,y,z) = "eps_0 * eps_r_air
+ eps_0 * (eps_r_alumina - eps_r_air)  * (z >= 0) * (z < sub_thick)"

macroscopic.mu_function(x,y,z) = "mu_0 * mu_r_air
+ mu_0 * (mu_r_alumina - mu_r_air)  * (z >= 0) * (z < sub_thick)"

##################################
############ FIELDS #############
#################################

###############
# domain size
# n_cellx/y/z and Lx/y/z are needed to calculate dx/dy/dz
###############
my_constants.n_cellx = 60
my_constants.n_celly = 1440
my_constants.n_cellz = 60
my_constants.max_grid_size = 60
my_constants.blocking_factor = 60

my_constants.Lx = 3.e-3
my_constants.Ly = 72.e-3
my_constants.Lz = 3.e-3


###############
# material properties
###############
my_constants.sigma_0 = 0.0
#my_constants.sigma_SiO2 = 1.e-10
my_constants.sigma_alumina = 1.e-12
my_constants.sigma_air = 1.e-15

my_constants.eps_0 = 8.8541878128e-12
#my_constants.eps_r_SiO2 = 3.8
my_constants.eps_r_alumina = 9.8
my_constants.eps_r_air = 1.0006

my_constants.mu_0 = 1.25663706212e-06
#my_constants.mu_r_SiO2 = 1.0
my_constants.mu_r_alumina = 1.0
my_constants.mu_r_air = 1.0
###############
###############
# waveguide port parameters
###############
my_constants.sub_thick = 5e-4
#my_constants.strip_width = 0.97518*sub_thick
my_constants.strip_width = 0.8*Lx

my_constants.freq = 10.e9

###############
# derived quantities and fundamental constants - don't touch these
###############
my_constants.pi = 3.14159265358979

# grid spacing
my_constants.dx = Lx / n_cellx
my_constants.dy = Ly / n_celly
my_constants.dz = Lz / n_cellz

my_constants.tiny = 1.e-12

#################################
#################################
############ Microstrip (Internal PEC) & Excitation #############
#################################
#################################
warpx.Apply_E_excitation_in_pml_region = 1

warpx.E_excitation_on_grid_style = parse_E_excitation_grid_function

warpx.Ex_excitation_flag_function(x,y,z) = "1* ((z > sub_thick-tiny) * (z < sub_thick+tiny) * (x > -strip_width/2.- tiny) * (x < strip_width/2. + tiny))"
warpx.Ex_excitation_grid_function(x,y,z,t) = "0."

warpx.Ey_excitation_flag_function(x,y,z) = "1* ((z > sub_thick-tiny) * (z < sub_thick+tiny) * (x > -strip_width/2.- tiny) * (x < strip_width/2. + tiny))"
warpx.Ey_excitation_grid_function(x,y,z,t) = "0."

warpx.Ez_excitation_flag_function(x,y,z) = "1* ((x > -strip_width/2.) * (x < strip_width/2.) * (z > 0.) * (z < sub_thick) * (y >= -dy/2.) * (y <= dy/2.))"
warpx.Ez_excitation_grid_function(x,y,z,t) = "sin(2*pi*freq*t)"

warpx.B_excitation_on_grid_style = parse_B_excitation_grid_function

warpx.Bx_excitation_flag_function(x,y,z) = "1* (x > -strip_width/2.) * (x < strip_width/2.) * (z > 0.) * (z < sub_thick) * (y >= 0.) * (y <= dy)"
warpx.By_excitation_flag_function(x,y,z) = "0."
warpx.Bz_excitation_flag_function(x,y,z) = "0"

warpx.Bx_excitation_grid_function(x,y,z,t) = "(mu_0*mu_r_alumina/50.) * sin(2*pi*freq*t)"
warpx.By_excitation_grid_function(x,y,z,t) = "0."
warpx.Bz_excitation_grid_function(x,y,z,t) = "0."
###############
# diagnostics
###############

diagnostics.diags_names = plt
#diagnostics.diags_names = plt input output

###############
# full plotfiles
plt.intervals = 400
plt.fields_to_plot = Ex Ey Ez Bx By Bz epsilon sigma mu
plt.diag_type = Full

########################## Surface Integral on the Y-Plane #############################
warpx.reduced_diags_names = ESurf_L1S1 ESurf_L2S1 ESurf_L3S1 ESurf_L4S1 ESurf_L5S1 ESurf_L6S1 ESurf_L7S1 ESurf_L8S1 ESurf_L9S1 ESurf_L10S1 ESurf_L11S1 ESurf_L12S1 ESurf_L13S1 ESurf_L14S1 ESurf_L15S1 ESurf_L16S1 ESurf_L17S1 ESurf_L18S1 ESurf_L19S1 ESurf_L20S1 ESurf_L21S1 ESurf_L22S1 ESurf_L23S1 ESurf_L24S1 ESurf_L25S1 ESurf_L26S1 ESurf_L27S1 ESurf_L28S1 ESurf_L29S1 ESurf_L30S1 ESurf_L31S1 ESurf_L32S1 ESurf_L33S1 ESurf_L34S1 ESurf_L35S1 ESurf_L36S1 ESurf_L37S1 ESurf_L38S1 ESurf_L39S1 ESurf_L40S1 ESurf_L41S1 BSurf_L1S1 BSurf_L1S2 BSurf_L1S3 BSurf_L1S4 BSurf_L2S1 BSurf_L2S2 BSurf_L2S3 BSurf_L2S4 BSurf_L3S1 BSurf_L3S2 BSurf_L3S3 BSurf_L3S4 BSurf_L4S1 BSurf_L4S2 BSurf_L4S3 BSurf_L4S4 BSurf_L5S1 BSurf_L5S2 BSurf_L5S3 BSurf_L5S4 BSurf_L6S1 BSurf_L6S2 BSurf_L6S3 BSurf_L6S4 BSurf_L7S1 BSurf_L7S2 BSurf_L7S3 BSurf_L7S4 BSurf_L8S1 BSurf_L8S2 BSurf_L8S3 BSurf_L8S4 BSurf_L9S1 BSurf_L9S2 BSurf_L9S3 BSurf_L9S4 BSurf_L10S1 BSurf_L10S2 BSurf_L10S3 BSurf_L10S4 BSurf_L11S1 BSurf_L11S2 BSurf_L11S3 BSurf_L11S4 BSurf_L12S1 BSurf_L12S2 BSurf_L12S3 BSurf_L12S4 BSurf_L13S1 BSurf_L13S2 BSurf_L13S3 BSurf_L13S4 BSurf_L14S1 BSurf_L14S2 BSurf_L14S3 BSurf_L14S4 BSurf_L15S1 BSurf_L15S2 BSurf_L15S3 BSurf_L15S4 BSurf_L16S1 BSurf_L16S2 BSurf_L16S3 BSurf_L16S4 BSurf_L17S1 BSurf_L17S2 BSurf_L17S3 BSurf_L17S4 BSurf_L18S1 BSurf_L18S2 BSurf_L18S3 BSurf_L18S4 BSurf_L19S1 BSurf_L19S2 BSurf_L19S3 BSurf_L19S4 BSurf_L20S1 BSurf_L20S2 BSurf_L20S3 BSurf_L20S4 BSurf_L21S1 BSurf_L21S2 BSurf_L21S3 BSurf_L21S4 BSurf_L22S1 BSurf_L22S2 BSurf_L22S3 BSurf_L22S4 BSurf_L23S1 BSurf_L23S2 BSurf_L23S3 BSurf_L23S4 BSurf_L24S1 BSurf_L24S2 BSurf_L24S3 BSurf_L24S4 BSurf_L25S1 BSurf_L25S2 BSurf_L25S3 BSurf_L25S4 BSurf_L26S1 BSurf_L26S2 BSurf_L26S3 BSurf_L26S4 BSurf_L27S1 BSurf_L27S2 BSurf_L27S3 BSurf_L27S4 BSurf_L28S1 BSurf_L28S2 BSurf_L28S3 BSurf_L28S4 BSurf_L29S1 BSurf_L29S2 BSurf_L29S3 BSurf_L29S4 BSurf_L30S1 BSurf_L30S2 BSurf_L30S3 BSurf_L30S4 BSurf_L31S1 BSurf_L31S2 BSurf_L31S3 BSurf_L31S4 BSurf_L32S1 BSurf_L32S2 BSurf_L32S3 BSurf_L32S4 BSurf_L33S1 BSurf_L33S2 BSurf_L33S3 BSurf_L33S4 BSurf_L34S1 BSurf_L34S2 BSurf_L34S3 BSurf_L34S4 BSurf_L35S1 BSurf_L35S2 BSurf_L35S3 BSurf_L35S4 BSurf_L36S1 BSurf_L36S2 BSurf_L36S3 BSurf_L36S4 BSurf_L37S1 BSurf_L37S2 BSurf_L37S3 BSurf_L37S4 BSurf_L38S1 BSurf_L38S2 BSurf_L38S3 BSurf_L38S4 BSurf_L39S1 BSurf_L39S2 BSurf_L39S3 BSurf_L39S4 BSurf_L40S1 BSurf_L40S2 BSurf_L40S3 BSurf_L40S4 BSurf_L41S1 BSurf_L41S2 BSurf_L41S3 BSurf_L41S4 EVolume1 BVolume1 


### Defining surfaces for E-field surface integrals to obtain voltages ###


# E-Surface ESurf_L1S1


ESurf_L1S1.type = RawEFieldReduction
ESurf_L1S1.reduction_type = integral
ESurf_L1S1.integration_type = surface
ESurf_L1S1.intervals = 1
ESurf_L1S1.surface_normal = y
my_constants.ESurf_L1S1_Loc = dy
ESurf_L1S1.scaling_factor = 2050.903 2050.903 2050.903
ESurf_L1S1.reduced_function(x,y,z) = " (x > -strip_width/2. - tiny) * (x < strip_width/2. + tiny) * (y > ESurf_L1S1_Loc - dy/2. - tiny ) * (y < ESurf_L1S1_Loc + tiny) * (z > 0.) * ( z < sub_thick + tiny) *1"


