################################
####### GENERAL PARAMETERS ######
#################################
max_step = 4000

amr.n_cell = n_cellx n_celly n_cellz
amr.max_grid_size = max_grid_size
amr.blocking_factor = blocking_factor
amr.refine_grid_layout = 1  # if n_MPI > n_grids, the grids will be successively divided in half until n_MPI <= n_grids

# need this here for some reason with blocking_factor = 40
amr.check_input = 0

geometry.dims = 3
geometry.prob_lo = -Lx_l 0. 0.
geometry.prob_hi =  Lx_r Ly Lz

amr.max_level = 0

# use pec instead of pml overlaying current source so you don't get a reflection
boundary.field_lo = pml pml pec
boundary.field_hi = pml pml pml

#################################
############ NUMERICS ###########
#################################
warpx.verbose = 1

warpx.cfl = 0.67298

# vacuum or macroscopic
algo.em_solver_medium = macroscopic

# laxwendroff or backwardeuler
algo.macroscopic_sigma_method = laxwendroff

###############
# geometry
# each row represents a different part of the circuit
# 1. air everywhere, then add in alumina
# 2. alumina at the bottom of the microstrip
# 2. metal above alumina (aluminum or copper)
###############

macroscopic.sigma_function(x,y,z) = "sigma_air
+ (sigma_di - sigma_air)   * (z >= 0) * (z < sub_thick)"
#+ (sigma_IPEC - sigma_air) * (y <= strip_ylength1) * (z > sub_thick-tiny) * (z < sub_thick+tiny) * (x > -strip_width/2.- tiny) * (x < strip_width/2. + tiny) 
#+ (sigma_IPEC - sigma_air) * (y > strip_ylength1) * (y <= strip_ylength1 + patch_y) * (z > sub_thick-tiny) * (z < sub_thick+tiny) * (x > - W1 - tiny) * (x < W2 + tiny) 
#+ (sigma_IPEC - sigma_air) * (y > strip_ylength1 + patch_y) * (z > sub_thick-tiny) * (z < sub_thick+tiny) * (x > offset -strip_width/2.- tiny) * (x < offset + strip_width/2. + tiny) "

macroscopic.epsilon_function(x,y,z) = "eps_0 * eps_r_air
+ eps_0 * (eps_r_di - eps_r_air)  * (z >= 0) * (z < sub_thick)"

macroscopic.mu_function(x,y,z) = "mu_0 * mu_r_air
+ mu_0 * (mu_r_di - mu_r_air)  * (z >= 0) * (z < sub_thick)"

##################################
############ FIELDS #############
#################################

###############
# domain size
# Input parameters are selected according to Zhang's paper.
###############
my_constants.dx=4.064e-4
my_constants.dy=4.233e-4 
my_constants.dz=2.6467e-4 
my_constants.n_cellx = 80
my_constants.n_celly = 100
my_constants.n_cellz = 16
my_constants.max_grid_size = 200
my_constants.blocking_factor = 200

my_constants.Lx = n_cellx*dx
my_constants.Ly = n_celly*dy
my_constants.Lz = n_cellz*dz

###############
# material properties
###############
my_constants.sigma_0 = 0.0
my_constants.sigma_di = 0.0
#my_constants.sigma_di = 0.0
my_constants.sigma_air = 0.0
#my_constants.sigma_IPEC = 1.0

my_constants.eps_0 = 8.8541878128e-12
my_constants.eps_r_di = 2.2
my_constants.eps_r_air = 1.0

my_constants.mu_0 = 1.25663706212e-06
my_constants.mu_r_di = 1.0
my_constants.mu_r_air = 1.0
###############
###############
# waveguide port parameters
###############
my_constants.Lx_l = 15*dx + patch_smallx + strip_width/2.
my_constants.Lx_r = Lx - Lx_l
my_constants.strip_width = 2.413e-3
my_constants.port_width = 6*dx
my_constants.strip_ylength1 = 50*dy 
my_constants.patch_y = 6*dy
my_constants.patch_xtot = 50*dx
my_constants.patch_smallx = 5.65e-3 
my_constants.patch_largex = patch_xtot - patch_smallx - strip_width 
my_constants.strip_ylength2 = Ly - strip_ylength1 - patch_y
my_constants.sub_thick = 3*dz 
my_constants.W1 = patch_smallx + strip_width/2.
my_constants.W2 = patch_largex + strip_width/2.
my_constants.offset = patch_xtot - 2*patch_smallx - strip_width

###############
# derived quantities and fundamental constants - don't touch these
###############
my_constants.T = 15e-12
my_constants.t0 = 3*T
my_constants.tiny = 1e-12

#################################
#################################
############ Microstrip (Internal PEC) & Excitation #############
#################################
#################################
warpx.Apply_E_excitation_in_pml_region = 1

warpx.E_excitation_on_grid_style = parse_E_excitation_grid_function

warpx.Ex_excitation_flag_function(x,y,z) = "1*  (y <= strip_ylength1) * (z > sub_thick-tiny) * (z < sub_thick+tiny) * (x > -strip_width/2.- tiny) * (x < strip_width/2. + tiny) + 1* (y > strip_ylength1) * (y <= strip_ylength1 + patch_y) * (z > sub_thick-tiny) * (z < sub_thick+tiny) * (x > - W1 - tiny) * (x < W2 + tiny) + 1* (y > strip_ylength1 + patch_y) * (z > sub_thick-tiny) * (z < sub_thick+tiny) * (x > offset -strip_width/2.- tiny) * (x < offset + strip_width/2. + tiny) "
warpx.Ex_excitation_grid_function(x,y,z,t) = "0."

warpx.Ey_excitation_flag_function(x,y,z) = "1*  (y <= strip_ylength1) * (z > sub_thick-tiny) * (z < sub_thick+tiny) * (x > -strip_width/2.- tiny) * (x < strip_width/2. + tiny) + 1* (y > strip_ylength1) * (y <= strip_ylength1 + patch_y) * (z > sub_thick-tiny) * (z < sub_thick+tiny) * (x > - W1 - tiny) * (x < W2 + tiny) + 1* (y > strip_ylength1 + patch_y) * (z > sub_thick-tiny) * (z < sub_thick+tiny) * (x > offset -strip_width/2.- tiny) * (x < offset + strip_width/2. + tiny) "
warpx.Ey_excitation_grid_function(x,y,z,t) = "0."

warpx.Ez_excitation_flag_function(x,y,z) = "2* ((x > -port_width/2.) * (x < port_width/2.) * (z > 0.) * (z < sub_thick) * (y >= -dy/2.) * (y <= dy/2.))"

warpx.Ez_excitation_grid_function(x,y,z,t) = "exp(-(t-t0)**2/(T**2))"

warpx.B_excitation_on_grid_style = parse_B_excitation_grid_function

warpx.Bx_excitation_flag_function(x,y,z) = "0."
warpx.By_excitation_flag_function(x,y,z) = "0."
warpx.Bz_excitation_flag_function(x,y,z) = "0"

warpx.Bx_excitation_grid_function(x,y,z,t) = "0."
warpx.By_excitation_grid_function(x,y,z,t) = "0."
warpx.Bz_excitation_grid_function(x,y,z,t) = "0."
###############
# diagnostics
###############

diagnostics.diags_names = plt
plt.intervals = 40
plt.fields_to_plot = Ex Ey Ez Bx By Bz epsilon sigma mu
plt.diag_type = Full
warpx.reduced_diags_names = ESurf_L1S1 ESurf_L2S1 ESurf_L3S1 ESurf_L4S1 ESurf_L5S1 ESurf_L6S1 ESurf_L7S1 ESurf_L8S1 ESurf_L9S1 ESurf_L10S1 ESurf_L11S1 ESurf_L12S1 BSurf_L1S1 BSurf_L1S2 BSurf_L1S3 BSurf_L1S4 BSurf_L2S1 BSurf_L2S2 BSurf_L2S3 BSurf_L2S4 BSurf_L3S1 BSurf_L3S2 BSurf_L3S3 BSurf_L3S4 BSurf_L4S1 BSurf_L4S2 BSurf_L4S3 BSurf_L4S4 BSurf_L5S1 BSurf_L5S2 BSurf_L5S3 BSurf_L5S4 BSurf_L6S1 BSurf_L6S2 BSurf_L6S3 BSurf_L6S4 BSurf_L7S1 BSurf_L7S2 BSurf_L7S3 BSurf_L7S4 BSurf_L8S1 BSurf_L8S2 BSurf_L8S3 BSurf_L8S4 BSurf_L9S1 BSurf_L9S2 BSurf_L9S3 BSurf_L9S4 BSurf_L10S1 BSurf_L10S2 BSurf_L10S3 BSurf_L10S4 BSurf_L11S1 BSurf_L11S2 BSurf_L11S3 BSurf_L11S4 BSurf_L12S1 BSurf_L12S2 BSurf_L12S3 BSurf_L12S4 EVolume1 BVolume1 


### Defining surfaces for E-field surface integrals to obtain voltages ###


# E-Surface ESurf_L1S1


ESurf_L1S1.type = RawEFieldReduction
ESurf_L1S1.reduction_type = integral
ESurf_L1S1.integration_type = surface
ESurf_L1S1.intervals = 1
ESurf_L1S1.surface_normal = y
my_constants.ESurf_L1S1_Loc = dy
ESurf_L1S1.scaling_factor = 410.105 410.105 410.105
ESurf_L1S1.reduced_function(x,y,z) = " (x > -port_width/2. - tiny) * (x < port_width/2. + tiny) * (y > ESurf_L1S1_Loc - dy/2. - tiny ) * (y < ESurf_L1S1_Loc + tiny) * (z > 0.) * ( z < sub_thick + tiny) *1"


